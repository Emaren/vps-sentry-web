{
  "name": "vps-sentry-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DD_TRACE_ENABLED=false next dev -H 127.0.0.1 -p 3036",
    "build": "DD_TRACE_ENABLED=false next build --webpack",
    "start": "DD_TRACE_ENABLED=false next start",

    "lint": "DD_TRACE_ENABLED=false NODE_OPTIONS='' eslint",
    "test": "DD_TRACE_ENABLED=false NODE_OPTIONS='' vitest run",
    "test:watch": "DD_TRACE_ENABLED=false NODE_OPTIONS='' vitest",
    "test:dd": "vitest run",
    "qa:baseline": "./scripts/qa-baseline.sh",

    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "generate:sqlite": "prisma generate --schema prisma/schema.prisma",
    "generate:postgres": "prisma generate --schema prisma/schema.postgres.prisma",

    "db:pg:init": "./scripts/db/postgres-init.sh",
    "db:pg:copy": "./scripts/db/sqlite-to-postgres-copy.sh",
    "db:pg:verify": "./scripts/db/sqlite-postgres-verify.sh",
    "db:pg:shadow": "./scripts/db/sqlite-postgres-shadow-read.sh",
    "db:pg:acceptance": "./scripts/db/sqlite-postgres-acceptance.sh",
    "db:pg:migrate": "./scripts/db/sqlite-to-postgres-migrate.sh",
    "db:pg:cutover": "./scripts/db/postgres-cutover.sh",
    "db:pg:rollback": "./scripts/db/postgres-rollback.sh",

    "ops:worker": "DD_TRACE_ENABLED=false node ./scripts/ops-worker.mjs",
    "ops:worker:once": "DD_TRACE_ENABLED=false node ./scripts/ops-worker.mjs --once",
    "ops:slo:check": "./scripts/vps-slo-burn-rate.sh --no-alert",
    "ops:slo:alert": "./scripts/vps-slo-burn-rate.sh --alert",
    "ops:rpo-rto:report": "./scripts/vps-rpo-rto-report.sh",
    "ops:rpo-rto:alert": "./scripts/vps-rpo-rto-report.sh --alert",

    "supplychain:check": "./scripts/supply-chain-check.sh --no-lock-verify",
    "supplychain:check:strict": "./scripts/supply-chain-check.sh --strict",
    "chaos:certify": "./scripts/chaos-certify.sh --remote",
    "chaos:certify:fast": "./scripts/chaos-certify.sh --remote --skip-restart",

    "sentinel:scorecard": "DD_TRACE_ENABLED=false node ./scripts/sentinel-prime-scorecard.mjs",
    "sentinel:scorecard:fast": "DD_TRACE_ENABLED=false node ./scripts/sentinel-prime-scorecard.mjs --fast",
    "sentinel:scorecard:strict": "DD_TRACE_ENABLED=false node ./scripts/sentinel-prime-scorecard.mjs --strict"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.2",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.12",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stripe": "^20.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }
}
